{
  "name": "Fussball",
  "version": "0.0.1",
  "description": "Fussball",
  "author": "Bing Steup <b.steup@sping.nl>",
  "private": true,
  "jest": {
    "verbose": false,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "globals": {
      "__TEST__": true
    },
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/config/",
      "/spec/",
      "databaseConnection.js"
    ]
  },
  "scripts": {
    "start": "nodemon app.js",
    "console": "node console.js",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:rollback": "node_modules/.bin/sequelize db:migrate:undo",
    "test": "node_modules/jest/bin/jest.js --runInBand --forceExit"
  },
  "dependencies": {
    "app-module-path": "^2.2.0",
    "jsonfile": "^2.4.0",
    "kcors": "next",
    "koa": "^2.1.0",
    "koa-body": "^1.6.0",
    "koa-bodyparser": "^4.1.0",
    "koa-logger": "^2.0.1",
    "koa-router": "next",
    "mysql": "^2.13.0",
    "raven": "^1.2.0",
    "sequelize": "^3.30.2",
    "sequelize-cli": "^2.5.1",
    "sequelize-to-json": "^0.10.1",
    "sequelize-tokenify": "^0.1.5"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.0",
    "babel-preset-stage-3": "^6.22.0",
    "faker": "^4.1.0",
    "jest": "^19.0.2",
    "nodemon": "^1.11.0",
    "shipit-deploy": "^2.4.0",
    "shipit-npm": "^0.2.0",
    "shipit-pm2": "^0.0.4",
    "shipit-shared": "^4.4.2",
    "sqlite": "^2.5.0"
  }
}
